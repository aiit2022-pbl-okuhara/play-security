create_table("users") {
  t.Column("id", "uuid", {primary: true, "default_raw": "gen_random_uuid()"})
  t.Column("last_name", "string", {"size": 128})
  t.Column("first_name", "string", {"size": 128})
  t.Column("last_name_kana", "string", {"size": 128})
  t.Column("first_name_kana", "string", {"size": 128})  
  t.Column("nickname", "string", {"size": 20})
  t.Column("email", "string", {"size": 255})
  t.Column("password_hash", "text", {"null": false})
}
